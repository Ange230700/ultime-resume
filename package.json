{
  "name": "ultime-resume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-experimental-webstorage next dev",
    "build": "cross-env NODE_OPTIONS=--no-experimental-webstorage next build",
    "start": "cross-env NODE_OPTIONS=--no-experimental-webstorage next start",
    "lint": "next lint",
    "prepare": "husky",
    "generate-pdf": "tsx src/generate-pdf.ts",
    "save": "npm run generate-pdf && git add . && npx commit-prompt && git push"
  },
  "dependencies": {
    "hast-util-sanitize": "^5.0.2",
    "next": "15.3.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.5",
    "puppeteer": "^24.7.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
